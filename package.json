{
  "name": "impersonated-webconnect",
  "version": "0.1.0",
  "description": "Impersonated-Webconnect is a package that allows you to build TCP/WSS connection with impersonation as chrome.",
  "main": "index.node",
  "scripts": {
    "test": "cargo test",
    "cargo-build": "cargo build --message-format=json > cargo.log",
    "cross-build": "cross build --message-format=json > cross.log",
    "postcargo-build": "neon dist < cargo.log",
    "postcross-build": "neon dist -m /target < cross.log",
    "debug": "npm run cargo-build --",
    "build": "npm run cargo-build -- --release",
    "cross": "npm run cross-build -- --release"
  },
  "author": "YT-Gh0st",
  "license": "ISC",
  "devDependencies": {
    "@neon-rs/cli": "0.1.73"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WhityGhost/Impersonated-webconnect.git"
  },
  "keywords": [
    "Impersonate",
    "chrome",
    "Rust",
    "npm",
    "nodejs",
    "request",
    "impersonation"
  ],
  "bugs": {
    "url": "https://github.com/WhityGhost/Impersonated-webconnect/issues"
  },
  "homepage": "https://github.com/WhityGhost/Impersonated-webconnect#readme"
}
